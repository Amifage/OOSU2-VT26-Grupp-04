<Window x:Class="Presentationslager.AllaMedlemmar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentationslager"
        mc:Ignorable="d" FontSize="18" FontFamily="Lucida Bright"
        Title="Alla medlemmar" Height="700" Width="800">
    <Grid>
        <DataGrid x:Name="MedlemmarDataGrid" AutoGenerateColumns="False" IsReadOnly="True" Margin="10">
            <DataGrid.Columns>
                <DataGridTextColumn Header="ID" Binding="{Binding MedlemID}" Width="50"/>
                <DataGridTextColumn Header="Namn" Binding="{Binding Namn}" Width="*"/>
                <DataGridTextColumn Header="E-post" Binding="{Binding Epost}" Width="*"/>
                <DataGridTextColumn Header="Telefon" Binding="{Binding Telefonnummer}" Width="*"/>
                <DataGridTextColumn Header="Nivå" Binding="{Binding Medlemsnivå}" Width="*"/>
                <DataGridTextColumn Header="Betalstatus" Binding="{Binding Betalstatus}" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
